# Neo4j Configuration
NEO4J_URI=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# PostgreSQL Configuration
POSTGRES_HOST=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=eads

# Pinecone Configuration
PINECONE_API_KEY=your_pinecone_api_key

# Weaviate Configuration
WEAVIATE_API_KEY=  # Optional, leave empty for development

# Ray Configuration
RAY_HEAD_PORT=6379
RAY_DASHBOARD_PORT=8265

# MLflow Configuration
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_REGISTRY_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=eads_experiment
MLFLOW_ARTIFACTS_URI=./mlruns

# DVC Configuration
DVC_REMOTE_NAME=eads-storage
DVC_REMOTE_URL=./dvc-storage  # Change to your preferred storage (s3://, gs://, etc.)
DVC_REMOTE_ACCESS_KEY=  # Optional, for cloud storage
DVC_REMOTE_SECRET_KEY=  # Optional, for cloud storage

# Application Configuration
DEBUG=true
LOG_LEVEL=INFO
